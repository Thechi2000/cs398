{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "cs398",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true,
        "sidecar": true,
        "scope": [
          {
            "name": "../binaries/iverilog",
            "sidecar": true
          },
          {
            "name": "../binaries/vvp",
            "sidecar": true
          }
        ]
      },
      "fs": {
        "all": true,
        "scope": [
          "**"
        ]
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "mp.lap.epfl",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "externalBin": [
        "../binaries/iverilog",
        "../binaries/vvp"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "cs398",
        "width": 800,
        "height": 600
      }
    ]
  }
}